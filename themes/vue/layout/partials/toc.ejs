<ul class="menu-root">
    <% type !== 'api' && site.pages.find({type: type, category: page.category, version: page.version}).sort('order').each(function (p) { %>
        <% var fileName = p.path.replace(/^.+?\/([\w-]+)\.html/, '$1') %>
        <% if (type === 'guide') { %>
            <% if (p.category.length && p.order === 1) { %>
                <li><h3><%- p.category %><%= p.version ? ' v' + p.version : '' %></h3></li>
                <% if (p.see_version) { %>
                    <li><a href="<%- url_for(p.see_version_link) %>" class="sidebar-link">Version <%- p.see_version %></a></li>
                <% } %>
                <% if (p.github) { %>
                    <li><a href="<%- p.github %>" class="sidebar-link">View on Github</a></li>
                <% } %>
            <% } %>
        <% } %>
        <% if (type === 'cookbook') { %>
            <% if (p.order === 1.1) { %>
                <li><h3>Vue Instance</h3></li>
            <% } %>
            <!-- Move cookbook sections here once they have -->
            <!-- at least one recipe -->
        <% } %>
        <li>
            <a href="<%- url_for(p.path) %>" class="sidebar-link<%- page.title === p.title ? ' current' : '' %><%- p.is_new ? ' new' : '' %>"><%- p.title %></a>
        </li>
    <% }) %>
</ul>
